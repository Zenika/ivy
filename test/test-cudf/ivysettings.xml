<ivysettings>
  <settings defaultResolver="cudf"/>
  <!--<caches defaultCacheDir="${basedir}/${dl}/cache">
    <cache name="local" basedir="${basedir}/${dl}/cache" />
  </caches>-->
  <resolvers>
    <cudf name="cudf" pattern="${dl}/[module]-[revision].[ext]" m2compatible="true">
