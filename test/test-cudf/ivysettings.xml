<ivysettings>
    <settings defaultResolver="cudf" defaultResolveMode="cudf"/>
    <typedef name="archivaMock" classname="org.apache.ivy.plugins.cudf.ArchivaClientMock"/>
    <parsers>
        <cudf-parser/>
    </parsers>
    <resolvers>
        <cudf name="cudf">
            <archivaMock/>
            <artifact pattern="file:test/test-cudf/repositories/jar/[organisation]-[module]_[revision].[ext]"/>
        </cudf>
    </resolvers>
</ivysettings>